{
    "name": "suitetea/modularlaravel",
    "description": "Allows you to organize Laravel code modularly.",
    "license": "MIT",
    "version": "0.5.3",
    "authors": [
        {
            "name": "Steven Wade",
            "email": "stevenwadejr@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*@dev|~5.0",
        "illuminate/events": "4.2.*@dev|~5.0",
        "illuminate/config": "4.2.*@dev|~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "SuiteTea\\ModularLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuiteTea\\ModularLaravel\\Test\\": "test"
        }
    }
}
